{
  "name": "announceit",
  "version": "1.0.0",
  "description": "A tool that helps broadcasting agencies receive and manage announcements",
  "main": "app.js",
  "scripts": {
    "test": "nyc mocha ./server/tests/ --exit",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./server -s",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node ./build/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Blaise-Shyaka/AnnounceIT.git"
  },
  "author": "Blaise P. Shyaka",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Blaise-Shyaka/AnnounceIT/issues"
  },
  "homepage": "https://github.com/Blaise-Shyaka/AnnounceIT/templates/",
  "devDependencies": {
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-node": "^4.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "mocha": "^7.0.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1"
  },
  "engines": {
    "node": "10.16.3",
    "npm": "6.9.0"
  }
}
